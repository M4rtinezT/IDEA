package day17;

public class Test {
	public static void main(String[] args) {
		int a = 12;
		String str = String.valueOf(a);
		System.out.println(str.length());
	}
}

class Solution {
    public int reverse(int x) {
        if(x > pow(2,31) -1 || x < -pow(2,31) || x ==0){
            return 0;
        }else{
            for(int i = 0 ; i < String.valueOf(x).length() ; i++){
                
            }
        }
    }
}
